package com.main;

public class Main {
// I will make a seperate class for the array and rendering method and simply call it here
	public static void main(String[] args) {
		System.out.println("hello world");
		
		//testing what arrays look like, delete when done 
		int[][][] grid = MineGridUtilities.GenerateMineGrid();
		
		grid[3][4][1] = 7;
		
		// Logging a 2D array to the console
		for (int i = 0; i < grid.length; i++) {
		    for (int j = 0; j < grid[i].length; j++) {
		        System.out.print(grid[i][j][1] + " ");
		    }
		    System.out.println(); // Move to the next line after printing each row
		}
	}
}

