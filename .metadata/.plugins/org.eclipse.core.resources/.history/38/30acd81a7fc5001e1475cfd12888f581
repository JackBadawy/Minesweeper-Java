package com.main;
// im make a class for the grid cells because i need to store chars in one index of the final demension of my grid array
// and since this is java it's propbably important to use oop as much as possible
public class Cell {
private int intValue;
private char charValue; 

//constructor
public Cell(int intValue, char charValue) {
	this.intValue = intValue;
	this.charValue = charValue;
}

public int getIntValue() {
	return intValue;
}

public void setIntValue(int intValue) {
	this.intValue = intValue;
}

public char getCharValue() {
	return charValue;
}

public void setCharValue(char charValue) {
	this.charValue = charValue;
}




}
